<%= form_with(url: sessions_path) do |form| %><!--responde a la ruta sessions por metodo post-->
  <div>
    <%= form.label :login %>
    <%= form.text_field :login %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>


  <div>
    <%= form.submit %>
  </div>
<% end %>
